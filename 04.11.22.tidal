  # verb 0.8 1 0.5 0.2



d6 $ stack [
  s "[east(3,8)]" # n 6
] # gain   (rrandf 1.0 1.2 8)
  # speed  (rrandf 0.5 0.8 4)

d7 $ stack [
  degBy 0.1 $ s "[hh27*16]" # cutoff (rperf 4500 14500 (1/16))
  , s "[hh*8]"
  , degBy 0.8 $ s "[east*4]" # n (irand 5) # cutoff (rperf 4500 6000 (1/16)) # gain 0.3
] # pan    (rperf 0.3 0.8 16)
  # gain   (rperf 0.8 1.2 8)
