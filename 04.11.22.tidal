hush

setcps (120/60/4)

d1 $ n "[0 4 14 20 8]?" # sound "alphabet"

let root="fs"

d1 $ n (scale "minor" $ "[<[0,2,4] [4,6,8] [3,5,7] [5,7,9]>]"|-7) |+n root
  # s "superchip"
  # voice (slow 16 $ sine * 0.5 + 0.2)
  # adsr "0.2:1:1:1"
  # gain 0.8
  # cutoff 4000
  # verb 0.8 1 0.5 0.2
  # cloudsmode 0
  # clouds      (rperf 0.1 0.8 0.5) (rperf 0.5 0.8 0.5) (rperf 0.8 1.0 0.5) (rperf 0.5 0.8 0.5)
  # cloudsblend (rperf 0.8 1.0 0.5) (rperf 0.2 0.8 0.5) (rperf 0.38 0.39 0.5) (rperf 0.5 0.8 0.5)


d2 $ stack [
  (arp "<up down>" $ n (scale "minor" $ "[<[0,2,4,6]*2 [4,6,1,3]*2 [3,5,7,2]*2 [5,7,2,4]*2>]") |+n root)
  ]
